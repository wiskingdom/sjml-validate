    <sourceInfo>
      <title>나를 좋아하는 남자 vs 내가 좋아하는 남자</title>
      <author>고명우</author>
      <publisher>네이버 블로그</publisher>
      <date>2016-01-12 21:22:15</date>
      <dateCrawl>2019-08-03 16:20:32</dateCrawl>
      <url>http://blog.naver.com/PostView.nhn?blogId=jstar4242&logNo=220595531201</url>
      <view>0</view>
