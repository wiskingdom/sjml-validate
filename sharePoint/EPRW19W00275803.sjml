    <sourceInfo>
      <title>나는 지금 네가 생각하는 사람을 알고 있다</title>
      <author>네티하비</author>
      <publisher>egloos</publisher>
      <date>2008-12-17 10:58:00</date>
      <dateCrawl>2019-08-02 16:49:24</dateCrawl>
      <url>http://netyhobby.egloos.com/4780288</url>
